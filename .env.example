# Keycase Agent Configuration
# Copy this file to .env and fill in your values

# Required: Base HTTP API URL
HTTP_URL=http://localhost:3000/api

# Required: AgentToken (get this from the Keycase platform)
# Must start with 'agt_'
AGENT_TOKEN=agt_your_token_here

# Required: Unique agent name within your organization
AGENT_NAME=my-agent-01

# Optional: Agent version
AGENT_VERSION=1.0.0

# Optional: Comma-separated list of capabilities
AGENT_CAPABILITIES=selenium,api

# Optional: Comma-separated list of tags for categorization
AGENT_TAGS=production,windows
